{
  "manifest_version": 2,
  "name": "GPT-4 Chat Changer",
  "version": "1.0",
  "description": "Automatically clicks GPT-4 option when opening a new chat.",
  "permissions": ["activeTab", "https://chat.openai.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
